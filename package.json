{
  "name": "structure",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "env-cmd": "^10.1.0",
    "husky": "^4.3.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "validator": "^13.1.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:fix": "eslint . --fix",
    "build-production": "./node_modules/.bin/env-cmd -f ./.env.production react-scripts build",
    "build-development": "./node_modules/.bin/env-cmd -f ./.env.development react-scripts build"
